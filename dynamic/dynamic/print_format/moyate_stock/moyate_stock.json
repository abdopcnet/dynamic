 "doctype": "Print Format",
 "font": "Default",
 "html": "\n\n {%- set company = frappe.get_doc(\"Company\", doc.company) -%}\n <div class=\"mb-4\">\n      <div class=\"col-xs-12 mb-3\" style=\"text-align: right\">\n        <img src=\"{{company.company_logo}}\" width=\"20%\" alt=\"\" />\n      </div>\n    </div>\n    <br/>\n    <br/>\n        \n    <div class=\"grid-container mt-2\">\n        <div class=\"grid-child purple\">\n            <div >{{_(\"purpose\")}}:{{doc.stock_entry_type}}</div>\n        </div>\n    \n        <div class=\"grid-child green\">\n            <div dir=\"auto\">{{_(\"Doc NO\")}}:{{doc.name}}</div>\n        </div>\n        \n         <div class=\"grid-child purple\">\n            <div dir=\"auto\">{{_(\"Transaction Date And Time\")}} : {{doc.posting_date}}</div>\n        </div>\n  \n    </div>\n\n  \n     <div class=\"grid-container_2\">\n        <div class=\"grid-child purple\">\n            <div class=\"green\">{{_(\"From Warhouse\")}} : {{doc.from_warehouse}}</div>\n        </div>\n    \n        <div class=\"grid-child green\">\n            <div class=\"blue\">{{_(\"Target Warhouse\")}} : {{doc.to_warehouse}}</div>\n        </div>\n        \n    </div>\n    \n    \n    <table class=\"table table-bordered\">\n      <thead>\n          <tr>\n          <td scope=\"col\">{{_(\"Number\")}}</td>\n          <td scope=\"col\">{{_(\"Item Code\")}}</td>\n          <td scope=\"col\">{{_(\"Item Name\")}}</td>\n          <td scope=\"col\">{{_(\"Quantity\")}}</td>\n        </tr>\n        {%for row in doc.items%}\n        <tr>\n            <td>{{row.idx}}</td>\n            <td>{{row.item_code}}</td>\n            <td>{{row.item_name}}</td>\n             <td>{{row.qty}}</td>\n        </tr>\n        \n        {%endfor%}\n      </thead>\n    </table>\n    \n\n    <div class=\"grid-container_2\">\n    \n        <div class=\"grid-child green\">{{_(\"Storekeeper's signature\")}} : </div>\n\n        <div class=\"grid-child green\">{{_(\"The recipient's signature\")}} : </div>\n\n    </div>\n\n\n\n\n\n\n\n\n\n\n",

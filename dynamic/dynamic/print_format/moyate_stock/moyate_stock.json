 "css": "\n\n\n.grid-container {\n    \n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    grid-gap: 20px;\n}\n\n.grid-container_2{\n    margin-top:7px;\n    display: grid;\n    grid-template-columns: 1fr 1fr ;\n    grid-gap: 20px;\n}\n",
 "custom_format": 1,
 "default_print_language": "",

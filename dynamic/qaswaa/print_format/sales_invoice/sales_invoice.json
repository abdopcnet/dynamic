class=\"col-xs-2 col-2 second\">\n               ..............\n            </div>\n        </div>\n    </div>\n    <div class=\"row\">\n            <table class=\"table table-bordered table-condensed\">\n            <thead>\n    \t\t\t\t<tr>\n    \t\t\t\t    <th style=\"width: 40px\" class=\"table-sr\">Sr</th>\n    \t\t\t\t    <!--<th style=\"width: 100px;\" class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">Img</th>-->\n    \t\t\t\t     <th style=\"width: 100px;\" class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">Item Name</th>\n     \t\t\t\t    <th style=\"width: 10px;\" data-fieldname=\"items\" data-fieldtype=\"Table\">Quantity</th>\n     \t\t\t\t    <th style=\"width: 90px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\">Rate</th>\n      \t\t\t\t    <th style=\"width: 10px;\" data-fieldname=\"items\" data-fieldtype=\"Table\">Discount</th>\n      \t\t\t\t    <th style=\"width: 100px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\">Rate After Discount</th>\n    \t\t\t\t    <th style=\"width: 100px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\">Amount</th>\n                        <th style=\"width: 100px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\">Taxes Amount</th>\n                        <th style=\"width: 100px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\">Total After Taxes</th>\n\n    \t\t\t\t</tr>\n    \t\t</thead>\n    \t\t\n    \t\t<tbody>\n    \t\t     {% for item in doc.items%}\n    \t\t\t    <tr>\n    \t\t\t        <td class=\"table-sr\">{{item.idx}}</td>\n    \t\t\t        <td class=\"table-sr\">{{item.item_name}}</td>\n    \t\t\t        <td class=\"table-sr\">{{item.qty}} </td>\n    \t\t\t        <td class=\"table-sr\"> {{frappe.utils.fmt_money(item.rate,currency=doc.currency)}}</td>\n    \t\t\t        <td class=\"table-sr\">{{item.discount_percentage}} </td>\n    \t\t\t        <td class=\"table-sr\">{{(item.rate * item.discount_percentage ) /100}} </td>\n    \t\t\t        <td class=\"table-sr\"> \n    \t\t\t        {% set amount = item.amount  %}\n    \t\t\t        {% if item.discount_percentage %}\n    \t\t\t            {% set amount = ((item.rate * item.discount_percentage ) /100) *item.qty %}\n    \t\t\t           {{ amount }}\n    \t\t\t        {% else %}\n    \t\t\t            {{ amount }}\n    \t\t\t        {% endif %}\n    \t\t\t\n    \t\t\t        </td>\n       \t\t\t        <td>\n       \t\t\t            {% set taxes = 0%}\n       \t\t\t        {% if item.item_tax_template %}\n           \t\t\t        {% set taxes = get_taxes_amount(item.item_tax_template) %}\n           \t\t\t        {{taxes}}\n   \t                    {% endif %}\n                        </td>\n                        <td>\n                            {{ amount + taxes }}\n                        </td>\n\n\n    \t\t\t    </tr>\n\t\t\t    {%endfor%}\n\t\t\t    <!--<tr>-->\n\t\t\t    <!--    <td colspan= \"4\" class=\"head\">Grand Total</td>-->\n\t\t\t    <!--    <td>{{doc.base_grand_total}}</td>-->\n\t\t\t    <!--</tr>-->\n\t\t\t    <!--<tr>-->\n\t\t\t    <!--    <td colspan= \"4\" class=\"head\">Taxes and Charges</td>-->\n\t\t\t    <!--    <td>{{doc.base_total_taxes_and_charges}}</td>-->\n\t\t\t    <!--</tr>-->\n\t\t\t    <tr>\n\t\t\t        <td colspan= \"8\" >Outstanding Amount</td>\n\t\t\t        <td>{{doc.outstanding_amount}}</td>\n\t\t\t    </tr>\n    \t    </tbody>\n            </table>\n    </div>\n</div>",

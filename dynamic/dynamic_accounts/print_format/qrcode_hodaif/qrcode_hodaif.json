 "html": "\n\n\n\n\n\n<div class=\"container\">\n\n    <script src=\"http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js\"></script>\n    {%- set qr_code_data = encode_invoice_data(doc.name) -%}\n    \n    \n     \n    <!--<div style = \"width : 30% ;\">-->\n    <!--    {% if letter_head and not no_letterhead -%}-->\n    <!--{% if print_settings.repeat_header_footer %}-->\n    <!--   <br>  <div class=\"letter-head\">{{ letter_head }}</div>-->\n    <!--{%- endif %}{%- endif %}-->\n    <!--</div>-->\n        <div>\n            <p class= \"text-center fw-bold\" style= \"font-size : 16px\">\n                <b>\u0641\u0627\u062a\u0648\u0631\u0629 \u0636\u0631\u064a\u0628\u064a\u0629 </b>\n            </p>\n        </div>\n    \n    <div  class=\"row\">\n        <div class=\"col-xs-8 col-sm-8 col-md-8 col-lg-8\">\n            <p>\n                <b>\u062a\u0627\u0631\u064a\u062e \u0627\u0635\u062f\u0627\u0631 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629 /:- </b>\n                {{doc.posting_date}}\n            </p>\n            <p>\n                <b>\u0631\u0642\u0645 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629 /:- </b>\n                {{doc.name}}\n            </p>\n            <p>\n                <b> \u0627\u0633\u0645 \u0627\u0644\u0645\u0648\u0631\u062f /:-  </b>\n                {{doc.customer}}\n            </p>\n            <p>\n                <b> \u0639\u0646\u0648\u0627\u0646 \u0627\u0644\u0645\u0648\u0631\u062f /:-  </b>\n                ( \u0627\u0644\u0631\u064a\u0627\u0636 -\u062d\u064a \u0627\u0644\u0645\u0644\u0632- \u0634\u0627\u0631\u0639 \u0635\u0644\u0627\u062d \u0627\u0644\u062f\u064a\u0646 \u0627\u0644\u0623\u064a\u0648\u0628\u064a )\n            </p>\n            <p>\n                <b>\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a \u0644\u0644\u0645\u0648\u0631\u062f  /:- </b>\n                300379179400003        \n            </p>\n                {% set customer_doc=frappe.get_doc(\"Customer\" , doc.customer)%}\n            <p>\n                <b>  \u0627\u0644\u0639\u0646\u0648\u0627\u0646 \u0627\u0644\u0639\u0645\u064a\u0644 /:- </b>\n                {% if customer_doc.customer_primary_address %}\n                    {{customer_doc.customer_primary_address}}\n                {% endif %}\n            </p>\n            \n            <p>\n                <b> \u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a \u0644\u0644\u0639\u0645\u064a\u0644 /:- </b>\n                {% if customer_doc.tax_id %}\n                    {{customer_doc.tax_id}}\n                {% endif %}\n            </p>\n        </div>\n\n        <div class=\"col-xs-4 col-sm-4 col-md-4 col-lg-4\">\n            <img src=\"/assets/dynamic/images/hodaif.jpeg\"  width='100px' height='100px'  style=\"margin-right: 10% !important\" >\n            <img\n                style=\"margin-right: 5% !important\"\n                id=\"barcode\"\n                src=\"https://chart.googleapis.com/chart?chs=200x200&amp;cht=qr&amp;chl={{qr_code_data}}\"\n                alt=\"\"\n                title=\"Sales Invoice\"\n                width=\"150\"\n                height=\"150\"\n                />\n        </div>\n    </div>\n\n\n\n         <div class=\"row\">\n                <table class=\"table table-bordered table-condensed\">\n                <thead>\n                        <tr>\n                            <th style=\"width: 40px\" class=\"table-sr\">{{_(\"\u0631\u0642\u0645 \u0627\u0644\u0645\u0633\u062a\u062e\u0644\u0635 / \u0623\u0645\u0631 \u062e\u062f\u0645\u0629\")}}</th>\n                             <th style=\"width: 100px;\" class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">  {{_(\" \u0631\u0642\u0645 \u0627\u0644\u0639\u0642\u062f\")}}</th>\n                        </tr>\n                </thead>\n                \n                <tbody>\n                        <tr>\n                            <td class=\"table-sr\">{{doc.abstract_number}}</td>\n                            <td class=\"table-sr\">{{doc.contract_number_}}</td>\n                    </tr>\n                </tbody>\n                </table>\n        </div>\n        <div class=\"row\">\n                <table class=\"table table-bordered table-condensed\">\n                <thead>\n                        <tr>\n                            <th style=\"width: 40px\" class=\"table-sr\">{{_(\"\u0645\")}}</th>\n                             <th style=\"width: 100px;\" class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">  {{_(\"\u0646\u0648\u0639 \u0627\u0644\u0639\u0645\u0644\")}}</th>\n                             <th style=\"width: 10px;\" data-fieldname=\"items\" data-fieldtype=\"Table\">\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u062a\u0648\u0631\u064a\u062f \u0627\u0644\u0641\u0639\u0644\u064a</th>\n                             <th style=\"width: 90px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\">\u0631\u0642\u0645 \u0623\u0645\u0631 \u0627\u0644\u0639\u0645\u0644</th>\n                              <th style=\"width: 100px;\" data-fieldname=\"items\" data-fieldtype=\"Table\">\u0627\u062c\u0645\u0627\u0644\u064a \u0627\u0644\u0645\u0628\u0644\u063a \u0642\u0628\u0644 \u0623\u064a \u062e\u0635\u0648\u0645\u0627\u062a \u0623\u0648 \u063a\u0631\u0627\u0645\u0627\u062a \u0628\u0627\u0644\u0631\u064a\u0627\u0644 </th>\n                              <th style=\"width: 100px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\"> \u0646\u0633\u0628\u0629 \u0627\u0644\u0636\u0631\u064a\u0628\u0629</th>\n                            <th style=\"width: 100px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\">\u0645\u0628\u0644\u063a \u0627\u0644\u0636\u0631\u064a\u0628\u0629 \u0628\u0627\u0644\u0631\u064a\u0627\u0644 \u0627\u0644\u0633\u0639\u0648\u062f\u0649</th>\n                            <th style=\"width: 100px;\"  data-fieldname=\"items\" data-fieldtype=\"Table\"> \u0627\u062c\u0645\u0627\u0644\u064a \u0627\u0644\u0645\u0628\u0644\u063a \u0634\u0627\u0645\u0644 \u0627\u0644\u0636\u0631\u064a\u0628\u0629 \u0628\u0627\u0644\u0631\u064a\u0627\u0644 \u0627\u0644\u0633\u0639\u0648\u062f\u0649</th>\n                        </tr>\n                </thead>\n                \n                <tbody>\n                     {% for item in doc.items%}\n                        <tr>\n                            <td class=\"table-sr\">{{item.idx}}</td>\n                            <td class=\"table-sr\">{{item.item_name}}</td>\n                            <td class=\"table-sr\">{{item.date_1}} </td>\n                            <td class=\"table-sr\">{{item.work_number}} </td>\n                            <td class=\"table-sr\"> {{item.amount}}</td>\n                              {% set taxes = 0%}\n                               {% if item.item_tax_template %}\n                                   {% set taxes = get_taxes_amount(item.item_tax_template) %}\n                               {% endif %}\n                               <td>\n                                   {{taxes}}\n                            </td>\n                            <td>\n                                {{ (item.amount * taxes) /100 }}\n                            </td>\n                            <td class=\"table-sr\"> \n                                {{ ( (item.amount * taxes) /100 ) + item.amount }}\n                            </td>\n                        </tr>\n                    {%endfor%}\n                    <tr>\n                        <td colspan= \"4\" >\u0627\u0644\u0645\u062c\u0645\u0648\u0639</td>\n                        <td>{{get_all_total_amount(doc.items)[0]}}</td>\n                        <td></td>\n                        <td>{{get_all_total_amount(doc.items)[1]}}</td>\n                        <td>{{get_all_total_amount(doc.items)[2]}}</td>\n    \n                    </tr>\n                    <tr>\n                        <td colspan= \"7\" style = \"text-align : center\">\u0627\u0644\u063a\u0631\u0627\u0645\u0627\u062a</td>\n                        <td> {{doc.deduction}}</td>\n                    </tr>\n                    <tr>\n                        <td colspan= \"7\">\u0627\u0644\u0645\u062c\u0645\u0648\u0639 \u0645\u0639 \u0627\u0644\u0636\u0631\u064a\u0628\u0629 \u0628\u0627\u0644\u0631\u064a\u0627\u0644 \u0627\u0644\u0633\u0639\u0648\u062f\u0649 </td>\n                        <td>  {{get_all_total_amount(doc.items)[2] - doc.deduction}}</td>\n                    </tr>\n                </tbody>\n                </table>\n        </div>\n        \n    \n        <div>\n           <b> \u0646\u0623\u0645\u0644 \u0645\u0646\u0643\u0645 \u0627\u0644\u062a\u0643\u0631\u0645 \u0628\u0635\u0631\u0641 \u0642\u064a\u0645\u0629 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629 \u0628\u0645\u0628\u0644\u063a {{get_all_total_amount(doc.items)[2] - doc.deduction}} \u0631\u064a\u0627\u0644 \u0633\u0639\u0648\u062f\u0649\n        </b>\n        </div>\n        <div  style = \"text-align : center\">\n            <b>\n            \u0648\u0644\u0643\u0645 \u062c\u0632\u064a\u0644 \u0627\u0644\u0634\u0643\u0631 \u0648\u0627\u0644\u062a\u0642\u062f\u064a\u0631              </b>\n        </div>\n        <div style=\"text-align:left\">\n            <b>\n             \u0634\u0631\u0643\u0629 \u0627\u0644\u062d\u0636\u064a\u0641 \u0644\u0644\u0645\u0642\u0627\u0648\u0644\u0627\u062a\n       </b>\n        </div>\n    </div>",
